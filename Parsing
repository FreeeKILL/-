#include <iostream>
#include <vector>
#include <fstream>
using namespace std;
int main()
{
int i;
int j;
ifstream a;
a.open("1.txt");
char b;
vector <char> c;
while (!a.eof())
{
a>>b;
c.push_back(b);
}
vector <string> d;
for (i=0;i!=c.size()-1;i++)
{
for (j=0;j!=i;j++)
{
if (c[j]=='|')
{
d.push_back("Separator");
break;
}
else if  ((c[j]=='@')and (c[j+1]=='=')) d.push_back("Literal");
else if ((c[j]>91)and (c[j]<122))d.push_back("Read");
}
}
for (i=0;i!=d.size();i++)
{
cout<<d[i]<<endl;
}
cout<<"HALT";
}
